{
  "name": "typecomp",
  "version": "0.2.0",
  "description": "TypeScript library for WCA competition scheduling and group assignments",
  "type": "module",
  "main": "./lib/index.ts",
  "types": "./lib/index.ts",
  "exports": {
    ".": {
      "import": "./lib/index.ts",
      "types": "./lib/index.ts"
    },
    "./api": {
      "import": "./lib/api/index.ts",
      "types": "./lib/api/index.ts"
    },
    "./presets": {
      "import": "./lib/api/presets/index.ts",
      "types": "./lib/api/presets/index.ts"
    }
  },
  "scripts": {
    "login": "bun run engine/login.ts",
    "typecheck": "bun run tsc --noEmit --skipLibCheck",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "example": "bun run scripts/examples/basic.ts",
    "test": "bun test"
  },
  "keywords": [
    "wca",
    "world-cube-association",
    "speedcubing",
    "competition",
    "scheduling",
    "groupifier",
    "wcif"
  ],
  "author": "Speedcubing Ireland",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SpeedcubingIreland/TypeComp"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@wca/helpers": "^1.1.6",
    "javascript-lp-solver": "^0.4.24",
    "linear-program-parser": "^1.0.11",
    "luxon": "^3.7.2",
    "openid-client": "^6.8.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/bun": "^1.3.5",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.7"
  }
}
